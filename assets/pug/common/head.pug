include ../mixins/mixin
doctype html
html
	head
		meta(charset='utf-8')
		meta(name='viewport', content='width=device-width, initial-scale=1.0')
		title
		link(media='all', rel='stylesheet', href='style.css')
		script(type='text/javascript', src='http://ajax.googleapis.com/ajax/libs/jquery/1.12.1/jquery.min.js')
		script(type='text/javascript').
			window.jQuery || document.write('<script src="js/jquery-1.12.1.min.js", defer><\/script>')
		script(type='text/javascript', src='js/jquery.main.js', defer)
		block js
	block body
		body
			#wrapper
				block content
